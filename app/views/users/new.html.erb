<h1>SignUp Here</h1>
<%= form_for(@user) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation, "Confirmation" %>
  <%= f.password_field :password_confirmation %>

  <%= f.label :home %>
  <%= f.text_field :home %>

  <%= f.label :work %>
  <%= f.text_field :work %>

   <%= f.submit "SgnUp" %>
<% end %>   




